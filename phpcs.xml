<?xml version="1.0"?>
<ruleset name="WordPress-PSR4">
	<description>PSR4-compatible WordPress ruleset.</description>

	<rule ref="WordPress">
		<!-- PSR4 -->
		<exclude name="WordPress.Files.FileName.InvalidClassFileName"/>
		<exclude name="WordPress.Files.FileName.NotHyphenatedLowercase"/>
	</rule>
</ruleset>